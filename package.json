  "description": "chrome-extension-for-clipboard",
  "main": "index.js",
  "scripts": {
    "start": "cross-env REACT_APP_ENV=dev node utils/webserver.js",
    "watch": "cross-env REACT_APP_ENV=stg webpack --config webpack/webpack.dev.js --watch",
    "build:dev": "cross-env REACT_APP_ENV=dev webpack --config webpack/webpack.prod.js",
    "build:stage": "cross-env REACT_APP_ENV=stg webpack --config webpack/webpack.prod.js",
    "build:prod": "cross-env REACT_APP_ENV=prod webpack --config webpack/webpack.prod.js",
    "build": "cross-env REACT_APP_ENV=stg webpack --config webpack/webpack.prod.js",
    "postbuild": "cross-env cp nginx.conf dist",
    "test": "npx jest --coverage --testResultsProcessor=jest-sonar-reporter --config jest.config.js",
    "style": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint:dry": "eslint . --ext .ts,.tsx --cache --cache-location ./.eslintcache/ --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --cache --cache-location ./.eslintcache/ --max-warnings 0",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
  },
