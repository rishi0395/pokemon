{
  "manifest_version": 3,
  "name": "My Assistant",
  "description": "A chrome extension to interact with my assistant application.",
  "version": "1.0",
  "key": "",
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "48": "logo_icon.png",
    "128": "logo_icon.png"
  },
  "action": {
    "default_icon": {
      "48": "logo_icon.png",
      "128": "logo_icon.png"
    },
    "default_title": "My Assistant"
  },
  "side_panel": {
    "default_path": "sidePanel.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/vendor.js", "js/content_script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["**/*", "*"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self'; object-src 'none'; frame-src 'none'; style-src 'self' 'unsafe-inline'; connect-src https://*.walmart.com;"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": ["sidePanel", "storage", "clipboardRead", "clipboardWrite"]
}
